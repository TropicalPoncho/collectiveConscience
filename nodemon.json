{
  "watch": [
    "app.js",
    "routes/",
    "services/",
    "models/",
    "utils/",
    "bin/www",
    "public/",
    "views/",
    "src/"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/",
    "*.test.js",
    "public/javascripts/graph/mundo.js",
    "public/javascripts/index.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "500",
  "legacyWatch": true,
  "polling": true,
  "verbose": true
} 